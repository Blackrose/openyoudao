function Getconfig(i)
{
var content=$('#dict_'+i).attr('rel');
localStorage.setItem('dict',content)
//alert("fdsfsd")
alert(document.getElementsByTagName("input")[1].getAttribute("value"))
localStorage.setItem('keyword',document.getElementsByTagName("input")[1].getAttribute("value"))
}
</script>
